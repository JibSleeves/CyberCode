{
  "name": "quonx-ide",
  "version": "1.0.0",
  "description": "Quonx - Autonomous Multi-Agent AI Coding IDE",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev:electron\" \"npm run dev:services\"",
    "dev:electron": "electron . --dev",
    "dev:services": "node src/services/start-services.js",
    "build": "electron-builder",
    "build:services": "node scripts/build-services.js",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": ["ide", "ai", "coding", "electron", "autonomous"],
  "author": "Quonx Development Team",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "@types/node": "^20.10.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "fastify": "^4.24.3",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.6.1",
    "monaco-editor": "^0.45.0",
    "codemirror": "^6.0.1",
    "tree-sitter": "^0.20.4",
    "tree-sitter-javascript": "^0.20.2",
    "tree-sitter-typescript": "^0.20.3",
    "tree-sitter-python": "^0.20.4",
    "faiss-node": "^0.5.1",
    "chromadb": "^1.7.3",
    "qdrant-js": "^1.7.0",
    "sentence-transformers": "^2.2.2",
    "transformers": "^4.36.0",
    "@huggingface/transformers": "^2.6.0",
    "llama-cpp-node": "^2.0.0",
    "ollama": "^0.4.7",
    "openai": "^4.20.1",
    "@google-cloud/aiplatform": "^3.12.0",
    "rdflib": "^2.2.31",
    "sparqljs": "^3.7.1",
    "sqlite3": "^5.1.6",
    "better-sqlite3": "^9.2.2",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "sharp": "^0.33.0",
    "canvas": "^2.11.2",
    "node-fetch": "^3.3.2",
    "ws": "^8.14.2",
    "chokidar": "^3.5.3",
    "simple-git": "^3.20.0",
    "nodegit": "^0.27.0",
    "dockerode": "^4.0.0",
    "kubernetes-client": "^10.2.1",
    "yaml": "^2.3.4",
    "glob": "^10.3.10",
    "mime-types": "^2.1.35",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "chalk": "^5.3.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^4.0.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "archiver": "^6.0.1",
    "unzipper": "^0.10.14",
    "node-cron": "^3.0.3",
    "systeminformation": "^5.21.20",
    "node-machine-id": "^1.1.12",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "keytar": "^7.9.0"
  },
  "build": {
    "appId": "com.quonx.ide",
    "productName": "Quonx IDE",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "node_modules/**/*",
      "models/**/*",
      "assets/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
